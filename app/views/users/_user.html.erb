<%= turbo_frame_tag dom_id(user) do %>
  <div id="<%= dom_id user %>">
    <p>
      <strong>Name:</strong>
      <%= user.name %>
    </p>

    <p>
      <strong>Email:</strong>
      <%= user.email %>
    </p>

    <p>
      <strong>Address:</strong>
      <%= user.address %>
    </p>

    <p>
      <strong>Contact:</strong>
      <%= user.contact %>
    </p>

  </div>
<% end %>
